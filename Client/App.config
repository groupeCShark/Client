<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
    </startup>
    <system.serviceModel>
      <client>
        <endpoint name="CSharkServiceEndpoint" address="net.tcp://localhost:8080/CSharkService" binding="netTcpBinding" bindingConfiguration="BindingConfiguration" contract="CSharkLibrary.ICSharkService">
          <identity>
            <servicePrincipalName value=""/>
          </identity>
        </endpoint>
      </client>
      

      <bindings>
        <netTcpBinding>
          <binding name="BindingConfiguration" transferMode="Buffered"/>
        </netTcpBinding>
      </bindings>
    </system.serviceModel>
</configuration>
